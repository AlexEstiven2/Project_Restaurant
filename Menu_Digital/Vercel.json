{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/Css/(.*)", "dest": "/Assets/Css/$1" },
    { "src": "/JavaScript/(.*)", "dest": "/Assets/JavaScript/$1" },
    { "src": "/Image/(.*)", "dest": "/Assets/Image/$1" },
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "src": "/", "dest": "/Pages/Menu_D.html" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}