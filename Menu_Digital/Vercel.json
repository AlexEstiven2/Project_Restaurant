{
  "version": 2,
  "builds": [
    {
      "src": "App/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/App/Pages/Menu_D.html"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "App/index.js"
    },
    {
      "src": "/Image/(.*)",
      "dest": "/App/Assets/Image/$1"
    },
    {
      "src": "/Assets/(.*)",
      "dest": "/App/Assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "App/index.js"
    }
  ]
}